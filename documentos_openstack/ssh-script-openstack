#!/bin/bash


echo 'no-port-forwarding,no-agent-forwarding,no-X11-forwarding,command="echo '\''Please login as the user \"great\" rather than the user \"root\".'\'';echo;sleep 10;exit 142" ssh-rsa  great@great' >> ~/.ssh/authorized_keys








echo 'no-port-forwarding,no-agent-forwarding,no-X11-forwarding,command="echo 'Please login as the user \"great\" rather than the user \"root\".';echo;sleep 10;exit 142" ssh-rsa  great@great' >> ~/.ssh/authorized_keys







echo 'no-port-forwarding,no-agent-forwarding,no-X11-forwarding,command="echo '\''Please login as the user \\"great\\" rather than the user \\"root\\".'\'';echo;sleep 10;exit 142" ssh-rsa  great@great' >> ~/.ssh/authorized_keys



echo 'no-port-forwarding,no-agent-forwarding,no-X11-forwarding,command="echo '\''Please login as the user \"great\" rather than the user \"root\".'\'';echo;sleep 10;exit 142" ssh-rsa  great@great' >> ~/.ssh/authorized_keys


no-port-forwarding,no-agent-forwarding,no-X11-forwarding,command="echo 'Please login as the user \"great\" rather than the user \"root\".';echo;sleep 10;exit 142" ssh-rsa  great@great
